<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="es.logongas.encuestas.modelo.seguridad.Privilege" >
        <id name="idPrivilege" >
            <generator class="identity" />
        </id>
        <property name="entityName" />
        <property name="create" column="createRow" />
        <property name="insert" column="insertRow" />
        <property name="update" column="updateRow" />
        <property name="delete" column="deleteRow" />
        <property name="read" column="readRow" />
        <property name="search" />
        <property name="namedSearch" />
        <property name="metadata" />               
    </class>
</hibernate-mapping>